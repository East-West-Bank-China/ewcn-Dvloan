    <connection>DVLOANDS</connection>
    <sql>select 0                        as id,
       b.ACCTYPE                as agrmt_type,
       b.ACCNO                  as agrmt_cd,
       'DEP'                    as parent_cd,
       b.ACCSEQ                 as sub_type,
       b.prodcod                    as prod_cd,
       d.CUSTNAM                as name,
       concat(CUSTCD1, CUSTCD2) as descp,
       substr(b.ACCNO, 1, 3)    as book_keeping_org,
       b.CUSTCOD                as client,
       substr(b.ACCNO, 1, 3)    as origin_org,
       '行内'                   as origin_channel,
       OPNDATE                  as entry_date,
       OPNDATE                  as trade_date,
       OPNDATE                  as value_date,
       CASE
           WHEN EXPDATE != '0' then EXPDATE
           else null end        as expiry_date,
       CASE
           WHEN CLSDATE != '0' then CLSDATE
           else null end        as close_date,
       b.tenor                    as tenor,
       '3'                      as tenor_type,
       b.RECSTS                 as status,
       CASE
           WHEN LASTDAT != '0' then LASTDAT
           else null end        as last_acr_date,

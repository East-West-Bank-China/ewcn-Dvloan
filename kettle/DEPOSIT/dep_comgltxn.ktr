    <connection>DV_CAS</connection>
    <sql>select REFNUMB,
       VERNUMB,
       PRODUCT,
       FNCTION,
       GLSERNO,
       GLNUMBR,
       DRCRIND,
       CCYCODE,
       GLAMOUN,
       LCYEQUI,
       VALDATE,
       FLRMKS,
       RELREFE,
       ACCTYPE,
       LCYRATE,
       ACUDBUI,
       ACCUNIT,
       DOMIBCH,
       INOUTSG,
       PROFCEN,
       GLTYPEN,
       CUSTCOD,
       ERRFLAG,
       BOOKPTY,
       APPDATE,
       APPUSER,
       APPIND,
       NOSTROI,
       BANKENT,
       ORIGCCY,
       ORIGAMT,
       CCYINDD,
       LAPPPU,
       FLSTSCD,
       TXBRCH,
       CUSBRCH,
       ACCOFR,
       CASHIND,
       RFVADAT,
       LOANTYP,
       CRLINE,
       DUEDATE,
       PFCCODE
from (-- accrual
         SELECT trim(A.agrmt_cd)                                                      REFNUMB,
       DENSE_RANK() OVER (PARTITION BY a.agrmt_cd ORDER BY a.dep_id)            VERNUMB,  
               'DEPDVDEP'                                                       PRODUCT,
               'DEPDVDEP'                                                       FNCTION,
                row_number() over (partition by a.dep_id order by a.dep_id)     GLSERNO,
                coa_code                                                        GLNUMBR,
                CASE
                    WHEN DRCR = 1 THEN 'DR'
                    WHEN DRCR = 2 THEN 'CR'
                    END                                                         DRCRIND,
                a.CURRENCY                                                      CCYCODE,
                AMOUNT                                                          GLAMOUN,
                CASE
                   WHEN G.TTMID IS NULL THEN AMOUNT  
                    WHEN a.CURRENCY = 'USD' THEN ROUND(AMOUNT * G.TTMID, 2)
                    WHEN a.CURRENCY = 'RMB' THEN AMOUNT
										ELSE ROUND(AMOUNT * G.TTMID, 2)
                    END                                                         LCYEQUI,
                DATE_FORMAT(d.procdat, '%Y%m%d')                                VALDATE,
                IFNULL(LEFT(ee.descrip, 35), ' ')                                FLRMKS,
                trim(A.agrmt_cd)                                                      RELREFE,
                ' '                                                             ACCTYPE,
                CASE
                    WHEN a.CURRENCY = 'USD' THEN G.TTMID
                    WHEN a.CURRENCY = 'RMB' THEN 1
										ELSE G.TTMID
                    END                                                         LCYRATE,
                'D'                                                             ACUDBUI,
                ' '                                                             ACCUNIT,
                c.book_keeping_org                                              DOMIBCH,
                ' '                                                             INOUTSG,
                ' '                                                             PROFCEN,
                ' '                                                             GLTYPEN,
                a.client                                                        CUSTCOD,
                'R'                                                             ERRFLAG,
                c.book_keeping_org                                              BOOKPTY,
                0                                                               APPDATE,
